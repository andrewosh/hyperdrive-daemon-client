{
  "name": "hyperdrive-daemon-client",
  "version": "2.0.0",
  "description": "A client library and CLI tool for interacting with the Hyperdrive daemon.",
  "main": "index.js",
  "scripts": {
    "test": "tape test/index.js"
  },
  "files": [
    "index.js",
    "lib/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hypercore-protocol/hyperdrive-daemon-client.git"
  },
  "keywords": [
    "hyperdrive",
    "daemon",
    "client"
  ],
  "author": "Andrew Osheroff <andrewosh@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hypercore-protocol/hyperdrive-daemon-client/issues"
  },
  "homepage": "https://github.com/hypercore-protocol/hyperdrive-daemon-client#readme",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.14.0",
    "@oclif/errors": "^1.2.2",
    "@oclif/plugin-autocomplete": "^0.1.5",
    "@oclif/plugin-help": "^2.2.3",
    "call-me-maybe": "^1.0.1",
    "cli-progress": "^3.4.0",
    "codecs": "^2.0.0",
    "globby": "^11.0.0",
    "hyperdrive": "git@github.com:hypercore-protocol/hyperdrive#hyperspace-port",
    "hyperdrive-schemas": "^1.9.0",
    "hyperspace": "git@github.com:andrewosh/hyperspace",
    "last-one-wins": "^1.0.4",
    "mirror-folder": "^3.0.1",
    "nanoresource-promise": "^1.2.2",
    "ora": "^4.0.3",
    "pump": "^3.0.0",
    "pumpify": "^2.0.0",
    "stream-collector": "^1.0.1",
    "streamx": "^2.6.0",
    "tmp-promise": "^3.0.2"
  },
  "oclif": {
    "commands": "./bin/commands",
    "bin": "hyperdrive",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete"
    ],
    "topics": {
      "debug": {
        "description": "Assorted debugging commands"
      },
      "cleanup": {
        "description": "Commands for deleting drives and clearing disk space"
      }
    }
  },
  "devDependencies": {
    "tape": "^5.0.1"
  }
}
